{
    "name": "dscheinah/sx-session",
    "description": "Simple session abstraction based on PSR-11 containers with scopes.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Sx\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sx\\SessionTest\\": "test/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
